{
  "name": "@esengine/editor",
  "version": "0.1.0",
  "type": "module",
  "description": "Web-based scene editor for ESEngine",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./src/styles/editor.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "clean": "rimraf dist",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "esbuild-wasm": "^0.27.2",
    "esengine": "workspace:*",
    "lucide": "^0.563.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^6.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@vitest/ui": "^2.1.8",
    "happy-dom": "^20.6.1",
    "jsdom": "^25.0.1",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.0",
    "rollup-plugin-dts": "^6.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.0",
    "vitest": "^2.1.8"
  },
  "author": "ESEngine Team",
  "license": "MIT"
}
